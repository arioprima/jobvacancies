{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@utils/*": ["src/utils/*"],
      "@styles/*": ["src/styles/*"],
      "@images/*": ["src/assets/images/*"],
      "@icons/*": ["src/assets/icons/*"],
      "@pages/*": ["src/pages/*"],
      "@layouts/*": ["src/layout/*"],
      "@context/*": ["src/context/*"],
      "@hooks/*": ["src/hooks/*"],
      "@config/*": ["src/config/*"],
      "@api/*": ["src/api/*"],
      "@lib/*": ["src/lib/*"],
      "@store/*": ["src/store/*"],
      "@router/*": ["src/router/*"],
      "@services/*": ["src/services/*"],
      "@constants/*": ["src/constants/*"],
      "@types/*": ["src/types/*"]
    }
  },
  "include": ["src"]
}
